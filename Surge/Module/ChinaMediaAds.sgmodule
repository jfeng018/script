ChinaMediaAds = type=http-response,pattern=^http:\/\/access\.if\.iqiyi\.com\/3f1\/cards\.iqiyi\.com\/(views_category\/3\.0\/category_home|views_home\/3\.0\/qy_home|waterfall\/3\.0\/feed)\?,requires-body=1,engine={{{Engine}}},script-path=https://raw.githubusercontent.com/tutuh/script/master/JS/ChinaMediaAds.js
ChinaMediaAds = type=http-response,pattern=^https:\/\/cards\.iqiyi\.com\/views_category\/3\.0\/(category_home|categorylib_content|film_hybrid)\?,requires-body=1,engine={{{Engine}}},script-path=https://raw.githubusercontent.com/tutuh/script/master/JS/ChinaMediaAds.js
ChinaMediaAds = type=http-response,pattern=^https:\/\/cards\.iqiyi\.com\/(views_home\/3\.0\/qy_home|waterfall\/3\.0\/feed)\?,requires-body=1,engine={{{Engine}}},script-path=https://raw.githubusercontent.com/tutuh/script/master/JS/ChinaMediaAds.js
